                <ul class="menu accordion-menu">
                    @{
                        if (ViewBag.Menu != null)
                        {
                            List<GatePass.ViewModels.MenuObject> menu = ViewBag.Menu as List<GatePass.ViewModels.MenuObject>;
                            foreach (GatePass.ViewModels.MenuObject item in menu)
                            {
                                string _droplink = "";

                                if (item.IsActive)
                                {
                                    _droplink = "active open";
                                }

                                if (item.Items != null)
                                {
                                    <li class="droplink @_droplink">
                                        <a href="@item.URL" class="waves-effect waves-button"><span class="menu-icon glyphicon @item.Icon"></span><p> @item.Name </p><span class="arrow"></span></a>
                                        <ul class="sub-menu">
                                            @{
                                                List<GatePass.ViewModels.MenuObject> submenu = item.Items;
                                                foreach (GatePass.ViewModels.MenuObject si in submenu)
                                                {
                                                    string _droplink_subitem = "";
                                                    //if (si.Items != null)
                                                    if (si.IsActive)
                                                    {
                                                        _droplink_subitem = "active";
                                                    }
                                                    <li class="@_droplink_subitem"><a href="@si.URL">@si.Name</a></li>
                                                }
                                            }
                                        </ul>
                                    </li>
                                                }
                                                else
                                                {
                                                    <li class="@_droplink"><a href="@item.URL" class="waves-effect waves-button"><span class="menu-icon glyphicon @item.Icon"></span><p>@item.Name</p></a></li>
                                                        }


                                                    }
                                                }

                    }
                </ul>
